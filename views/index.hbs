    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
  <link rel="stylesheet" href="static/css/style.css">
</head>

<body>
  {{> navbar data=details}}
  {{> slider sliderData=slider}}

  {{!-- Banner 1 --}}

  <section class="banner1 background">
    <div class="container py-5 ">

       <div class="row">
      <div class="col-md-12 text-center">
        <h1>Start Your Journey in Web3</h1>
        <p class="text-center">Lorem ipsum dolor sit amet consectetur adipisicing elit. Accusamus ad provident ?</p>
        <button class="btn btn-dark text-center">Start</button>
      </div>
    </div>
    </div>
   
  </section>

  {{!-- Cards sevices --}}

  <section id="services_section" class="py-4 background" >

    <div class="container">

      <h1 class="text-center   text-secondary">Services offered</h1>

      <div class="row">

        {{!-- 1st card --}}
        {{#each cards}}
        <div class="col-md-4 py-2 px-4">
          <div class="card w-120 h-60 ">
            <div class="card-body">
              <div class="container text-center all_icon">
                <i class="fa-brands fa-servicestack "></i>
              </div>

              <h2 class="card-title">{{this.cardTitle}}</h1>
                <p class="card-text">{{this.cardText}}</p>
                <button>{{this.button1}}</button>
                <button>{{this.button2}}</button>
            </div>
          </div>
        </div>

        {{/each}}






      </div>


    </div>

  </section>

  {{!-- BAnner 2 --}}

  <section class="banner2 background" style="
    padding-bottom: 10px;
">
    <div class="container py-6">
      <div class="row">
        <div class="col-md-6 ">
          <h3>Security of web3</h3>
    <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptatibus sit expedita saepe nobis odio aut aliquid ea debitis labore perferendis in laudantium, sapiente recusandae officiis ipsa deserunt deleniti eos dolorum.</p>
  <button class="btn btn-dark">Get Secured</button>
        </div>
        <div class="col-md-6 py-6 px-10">  
          <img src="static/images/ill1.jpg" class="img-fluid"  width="200px" height="200px" alt="">

        </div>
      </div>
    </div>
  </section>

  {{!-- Banner 3 --}}

  <section class="banner2 py-10 background " style="
    padding-bottom: 10px;
">
    <div class="container py-6">
      <div class="row">

        <div class="col-md-6 py-6 px-10">  
          <img src="static/images/8401.jpg" class="img-fluid"  width="200px" height="200px" alt="">

        </div>


        <div class="col-md-6 py-6">
          <h3>Proof Of Stake and Work</h3>
    <p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Voluptatibus sit expedita saepe nobis odio aut aliquid ea debitis labore perferendis in laudantium, sapiente recusandae officiis ipsa deserunt deleniti eos dolorum.</p>
  <button class="btn btn-dark py-10">Know more</button>
        </div>
        
      </div>
    </div>
  </section>


  {{!-- Contact us section --}}

  <section id="contact" class="py-10 contactBackground" style="
    padding-bottom: 10px;
">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
           <h1 class="text-center  py-10 text-secondary">Contact Us</h1>
          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label text-center">Email address</label>
              <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>

            <div class="mb-3">
              <label for="number-data" class="form-label text-center">Your contact no.</label>
              <input name="number" type="number" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
              <div id="num" class="form-text">We'll never share your <noframes></noframes> with anyone else.</div>
            </div>


            <div class="form-floating mb-3 mt-4">
              <textarea name="comment" class="form-control" placeholder="Leave a comment here" id="floatingTextarea2"
                style="height: 100px"></textarea>
              <label for="floatingTextarea2">Comments</label>
            </div>

            <div class="text-center">
              <button type="submit" class="btn btn-primary btn-dark">Submit</button>
            </div>
          </form>

        </div>

      </div>
    </div>
  </section>

  {{!-- About us footer --}}

  <section class="aboutBackground">
    <div class="container py-5 text-center ">
      <div class="row">
        <div class="col-md-12">
